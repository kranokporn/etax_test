<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_printing_form" model="ir.ui.view">
        <field name="name">account.printing.form.inherit</field>
        <field name="model">account.printing</field>
        <field name="inherit_id" ref="docsign_account_printing.view_printing_form" />
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="action_call_service" string="Sign PDF" type="object" states="draft" class="oe_highlight"/>
            </xpath>
        </field>
    </record>
</odoo>
